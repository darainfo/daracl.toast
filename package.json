{
  "name": "dara-toast",
  "version": "0.1.16",
  "sideEffects": false,
  "description": "Dara Toast Message",
  "author": {
    "name": "darainfo",
    "email": "darainfosys@gmail.com",
    "url": "https://www.moaview.com"
  },
  "license": "MIT",
  "keywords": [
    "daraToast",
    "toast",
    "darainfo",
    "message",
    "notification",
    "javascript"
  ],
  "main": "src/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/darainfo/toast",
  "bugs": {
    "url": "https://github.com/darainfo/toast/issues",
    "email": "darainfosys@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/darainfo/toast.git"
  },
  "files": [
    "dist",
    "src",
    "style"
  ],
  "dependencies": {
    "esbuild": "^0.18.11",
    "typescript": "^5.1.6"
  },
  "prettier": {
    "printWidth": 80,
    "useTabs": false,
    "tabWidth": 2,
    "bracketSpacing": true,
    "semi": true,
    "singleQuote": false
  },
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/parser": "^7.21.4",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "babel-loader": "^8.3.0",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.40.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^9.0.0",
    "prettier": "^2.8.8",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-node": "^10.9.1",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "prepack": "yarn build",
    "build": "yarn build:prod && yarn scss-compile && yarn build:tsc && yarn build:js",
    "build:tsc": "npx tsc --emitDeclarationOnly",
    "build:js": "node build.js",
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js && webpack --env=mode=deploy --config webpack.dev.js",
    "dts:gen": "npx -p typescript tsc src/index.js --declaration --allowJs --emitDeclarationOnly --outDir dist",
    "test": "jest",
    "start": "webpack-dev-server --open --config ./webpack.dev.server.js",
    "scss-compile": "node-sass -r \"style\" -o \"dist\" --output-style compressed"
  }
}
